# R can be used as a calculator
2+3

sonia＜-3+2 # I assign the result of the operation to an object
sonia

lea＜-3+4
lea

sonia+lea
sonia^lea


tommazo ＜- c(10,20,30,50,70) # this is an array
erica＜-c  (1,3,5,7,9) 
plot(tomazo, erica)
plot(tomazo, erica, pch=19)
# pch is changing the point character

# changing the colour
plot (tomazo, erica, pch=19, col="blue", cex=2, xlab="cannabis", ylab="dolphine")
